{
  "name": "reactexpress",
  "version": "1.0.0",
  "description": "Boiler plate for react and express",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "starter":"node server.js",
    "server":"nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Norman Potts",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^3.6.1",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "nodemon": "^1.18.3"
  }
}
